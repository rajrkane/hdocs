{
  "name": "hdocs",
  "productName": "hdocs",
  "version": "1.0.0",
  "description": "A collaborative rich-text editing desktop application using Electron",
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint src --color",
    "backend-dev": "nodemon server/index.js --exec babel-node --presets es2015,stage-2",
    "backend-build": "babel server -d server-dist --presets es2015,stage-2",
    "backend-serve": "node server-dist/index.js"
  },
  "keywords": [],
  "author": "rajrk",
  "license": "MIT",
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "npm",
        "ignore": [
          "server",
          "server-dist"
        ]
      },
      "electronWinstallerConfig": {
        "name": "hdocs"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "",
        "name": ""
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "hdocs"
      }
    }
  },
  "dependencies": {
    "@material-ui/core": "^1.4.1",
    "@material-ui/icons": "^2.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.3",
    "draft-js": "^0.10.5",
    "draft-js-color-picker": "^1.0.2",
    "electron-compile": "^6.4.3",
    "electron-squirrel-startup": "^1.0.0",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "immutable": "^3.8.2",
    "material-ui": "^0.20.1",
    "mongoose": "^5.2.5",
    "morgan": "^1.9.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "socket.io-client": "^2.1.1",
    "typeface-roboto": "0.0.54"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "electron-forge": "^5.2.2",
    "electron-prebuilt-compile": "2.0.4",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.10.0",
    "nodemon": "^1.18.3",
    "socket.io": "^2.4.0"
  }
}
